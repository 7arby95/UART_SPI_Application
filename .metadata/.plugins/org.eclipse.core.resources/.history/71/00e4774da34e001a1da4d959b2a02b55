#include "stdio.h"
#include "string.h"
#include <stdlib.h>

int stringToInteger(char* str)
{
	int i;
	unsigned char size = strlen(str);
	unsigned char num = 0;

	for(i=0 ; i<size ; i++)
	{
		num = num * 10 + (str[i]-'0') ;
		printf("%d", num);
	}

	return num;
}

atoi()

int main(void)
{
	int x = atoi("400");

	printf("%d", x);

	return 0;
}
