#include "stdio.h"
#include "string.h"

int stringToInteger(char* str)
{
	int i;
	unsigned char size = strlen(str);
	unsigned char num = 0;

	for(i=0 ; i<size ; i++)
	{
		num = num * 10 + (str[i]-'0') ;
		printf("%c\n", str[i]);
	}

	return num;
}

int main(void)
{
	char* str = "400";
	int ex= stringToInteger(str);

	printf("%d", ex);

	return 0;
}
